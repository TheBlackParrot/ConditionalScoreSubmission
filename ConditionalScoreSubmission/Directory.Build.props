<?xml version="1.0" encoding="utf-8"?>
<!-- This file contains project properties used by the build. -->
<Project>

  <!-- Plugin Metadata -->
  <PropertyGroup>
    <PluginId>ConditionalScoreSubmission</PluginId>
    <PluginName>ConditionalScoreSubmission</PluginName>
    <Authors>TheBlackParrot</Authors>
    <Version>0.0.1</Version>
    <GameVersion>1.39.1</GameVersion>
    <Description>Adds conditions that must be met before scores submit to leaderboards</Description>
    <ProjectSource>https://github.com/TheBlackParrot/ConditionalScoreSubmission</ProjectSource>
    <ProjectHome>https://github.com/TheBlackParrot/ConditionalScoreSubmission</ProjectHome>
    <DonateUrl>https://ko-fi.com/septilateral</DonateUrl>
  </PropertyGroup>

  <!-- Plugin Dependencies -->
  <ItemGroup>
    <DependsOn Include="BSIPA" Version="^4.3.6"/>
    <DependsOn Include="BeatSaberMarkupLanguage" Version="^1.12.4"/>
    <DependsOn Include="SiraUtil" Version="^3.2.1"/>
  </ItemGroup>
  
  <PropertyGroup>
    <BSMTProjectType>BSIPA</BSMTProjectType>
    <GenerateManifest>true</GenerateManifest>
    <DisableZipRelease>true</DisableZipRelease>
  </PropertyGroup>
  
</Project>